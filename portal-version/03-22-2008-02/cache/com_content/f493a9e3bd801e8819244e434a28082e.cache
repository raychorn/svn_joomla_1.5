a:3:{s:4:"body";s:15741:"	<script language="javascript" type="text/javascript">	
		
		function countlengthcomment(field, countFieldId, maxlimit) {
			if (document.getElementById){
				target = document.getElementById(countFieldId);
				if (field.value.length > maxlimit){ // if too long...trim it!
					field.value = field.value.substring(0, maxlimit);
					// otherwise, update 'characters left' counter
				} else {
					target.innerHTML = "Available characters: " +  (maxlimit - field.value.length);
				}
			}
		}

		
		function showInputField(field, relative, maxlimit) {
			if (document.getElementById){
				target = document.getElementById(field);	
				target.style.display = "block";
				target.innerHTML = "Available characters: " + (maxlimit - relative.value.length);		
			}		
		
		}

	</script>	

